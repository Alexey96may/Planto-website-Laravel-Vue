{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "vueIndentScriptAndStyle": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^vue$", "^@inertiajs/(.*)$", "<THIRD_PARTY_MODULES>", "^@/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
